<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet ../default.css?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<GridPane alignment="CENTER" hgap="10.0" prefHeight="-1.0" prefWidth="-1.0" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
          fx:controller="client.controller.SetupBar">
    <children>
        <StackPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="580.0" prefWidth="900.0"
                   styleClass="dark-gradientpane">
            <children>
                <VBox minHeight="-Infinity" minWidth="-Infinity" prefWidth="900.0">
                    <children>
                        <AnchorPane prefHeight="120.0" prefWidth="900.0">
                            <children>
                                <Text fill="WHITE" layoutX="104.0" layoutY="4.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="Create Your Bar" textAlignment="LEFT" textOrigin="TOP">
                                    <font>
                                        <Font size="44.0"/>
                                    </font>
                                </Text>
                                <Button fx:id="addButton" layoutX="0.0" layoutY="4.0" mnemonicParsing="false"
                                        onAction="#addPump" styleClass="std-button" text="+" textFill="WHITE"/>
                                <Button fx:id="removeButton" layoutX="36.0" layoutY="4.0" mnemonicParsing="false"
                                        onAction="#removePump" styleClass="std-button" text="-" textFill="WHITE"/>
                            </children>
                        </AnchorPane>
                        <FlowPane id="" fx:id="pumpLayout" alignment="TOP_LEFT" columnHalignment="LEFT" hgap="150.0"
                                  minWidth="-Infinity" orientation="VERTICAL" prefHeight="422.0" prefWidth="900.0"
                                  prefWrapLength="560.0" rowValignment="TOP" vgap="30.0">
                            <padding>
                                <Insets left="40.0"/>
                            </padding>
                        </FlowPane>
                        <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="900.0">
                            <children>
                                <Button layoutX="838.0" layoutY="5.0" mnemonicParsing="false" onAction="#gotoHome"
                                        text="Save"/>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
